{
  "ten": {
    "log": {
      "handlers": [
        {
          "emitter": {
            "config": {
              "stream": "stdout"
            },
            "type": "console"
          },
          "formatter": {
            "colored": true,
            "type": "plain"
          },
          "matchers": [
            {
              "level": "info"
            }
          ]
        }
      ]
    },
    "predefined_graphs": [
      {
        "auto_start": true,
        "graph": {
          "connections": [
            {
              "cmd": [
                {
                  "names": [
                    "on_user_joined",
                    "on_user_left"
                  ],
                  "source": [
                    {
                      "extension": "agora_rtc"
                    }
                  ]
                },
                {
                  "names": [
                    "tool_register"
                  ],
                  "source": [
                    {
                      "extension": "weatherapi_tool_python"
                    }
                  ]
                }
              ],
              "data": [
                {
                  "name": "asr_result",
                  "source": [
                    {
                      "extension": "stt"
                    }
                  ]
                }
              ],
              "extension": "main_control"
            },
            {
              "audio_frame": [
                {
                  "dest": [
                    {
                      "extension": "streamid_adapter"
                    }
                  ],
                  "name": "pcm_frame"
                },
                {
                  "name": "pcm_frame",
                  "source": [
                    {
                      "extension": "tts"
                    }
                  ]
                }
              ],
              "data": [
                {
                  "name": "data",
                  "source": [
                    {
                      "extension": "message_collector"
                    }
                  ]
                }
              ],
              "extension": "agora_rtc"
            },
            {
              "audio_frame": [
                {
                  "dest": [
                    {
                      "extension": "stt"
                    }
                  ],
                  "name": "pcm_frame"
                }
              ],
              "extension": "streamid_adapter"
            }
          ],
          "nodes": [
            {
              "addon": "agora_rtc",
              "extension_group": "default",
              "name": "agora_rtc",
              "property": {
                "app_certificate": "${env:AGORA_APP_CERTIFICATE|}",
                "app_id": "${env:AGORA_APP_ID}",
                "channel": "agora_a5sygk",
                "enable_agora_asr": false,
                "publish_audio": true,
                "publish_data": true,
                "remote_stream_id": 170997,
                "stream_id": 0,
